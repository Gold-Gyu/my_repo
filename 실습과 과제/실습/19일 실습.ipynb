   "source": [
    "# 피보나치\n",
    "\n",
    "def fib_loop(n):\n",
    "    if n < 2:\n",
    "        print(n)\n",
    "\n",
    "    result = [0, 1]\n",
    "    for i in range(2, n+1):\n",
    "        result.append(result[i - 1] + result[i - 2])\n",
    "\n",
    "    return result[-1]\n",
    "\n",
    "fib_loop(10)"
   ]
  }
