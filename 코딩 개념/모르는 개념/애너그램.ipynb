   "source": [
    "words = ['eat', 'tea', 'tan', 'ate', 'nat', 'bat']\n",
    "\n",
    "\n",
    "anagrams = {}\n",
    "\n",
    "\n",
    "\n",
    "for word in words:\n",
    "\n",
    "    temp = sorted(word)\n",
    "    temp = \"\".join(temp)\n",
    "\n",
    "\n",
    "    if anagrams.get(temp):\n",
    "        anagrams[temp].append(word)\n",
    "    else:\n",
    "        anagrams[temp] = [word]\n",
    "\n",
    "print(anagrams.values())"
   ]
  }
